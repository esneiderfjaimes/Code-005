<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="96dp"
            android:height="96dp"
            android:viewportWidth="48"
            android:viewportHeight="48">
            <group android:name="anim">
                <group
                    android:name="fix"
                    android:translateX="11"
                    android:translateY="13">
                    <path
                        android:name="path"
                        android:fillColor="#00000000"
                        android:pathData="M 14 10 L 11 13 L 14 18 L 18 6 L 6 10 L 11 13"
                        android:strokeWidth="1"
                        android:strokeColor="#fff"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="anim">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="@integer/iconAnimationTime"
                    android:interpolator="@android:anim/overshoot_interpolator"
                    android:propertyName="translateX"
                    android:valueFrom="-14"
                    android:valueTo="0"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="@integer/iconAnimationTime"
                    android:interpolator="@android:anim/overshoot_interpolator"
                    android:propertyName="translateY"
                    android:valueFrom="14"
                    android:valueTo="0"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="10"
                android:interpolator="@android:anim/decelerate_interpolator"
                android:propertyName="strokeAlpha"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
</animated-vector>
